								"sink": {
									"name": "CommissionPct",
									"type": "Decimal",
									"physicalType": "smallmoney",
									"scale": 4,
									"precision": 10
								}
							},
							{
								"source": {
									"name": "SalesYTD",
									"type": "Decimal",
									"physicalType": "money",
									"scale": 4,
									"precision": 19
